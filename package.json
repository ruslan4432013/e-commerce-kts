{
  "name": "e-commerce-kts",
  "license": "ISC",
  "version": "1.0.0",
  "private": true,
  "browserslist": [
    ">0.2%",
    "not dead"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "start:webpack": "cross-env NODE_ENV=development webpack --mode=development",
    "start:server": "wait-on dist/server.js && node dist/server.js",
    "dev": "yarn run clean && npm-run-all --print-label --parallel start:webpack start:server",
    "start": "nodemon --exec yarn run dev --watch src/server --ext ts,tsx,json ",
    "build": "yarn run clean  && cross-env NODE_ENV=production webpack --mode production",
    "build:report": "yarn run build --withReport",
    "run": "node dist/server.js",
    "start:static": "yarn run clean  && cross-env NODE_ENV=development NO_SSR=true webpack serve --mode development --open",
    "build:static": "yarn run clean  && cross-env NODE_ENV=production NO_SSR=true webpack --mode production",
    "build:static:report": "yarn run build:static --withReport",
    "lint": "eslint . --fix --ext .ts,.tsx,.js",
    "ts-check": "tsc --skipLibCheck --noEmit --project tsconfig.json"
  },
  "devDependencies": {
